{
    "manifest_version": 3,
    "name": "Security URL Analyzer",
    "version": "1.0",
    "description": "웹사이트 보안 분석 확장 프로그램",
    "permissions": ["activeTab", "tabs", "scripting"],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      }
    }
  }
  